<view class="main"><view data-event-opts="{{[['tap',[['toView',['head_image?type=2']]]]]}}" class="header" bindtap="__e"><image class="header_image" style="width:100%;height:100%;" src="{{user.backImage}}" data-event-opts="{{[['tap',[['toView',['head_image?type=2']]]]]}}" bindtap="__e"></image><view class="head"><image class="head_icon" src="{{user.headImage}}" data-event-opts="{{[['tap',[['toView',['head_image?type=1']]]]]}}" bindtap="__e"></image><text data-event-opts="{{[['tap',[['toView',['username']]]]]}}" class="head_font" bindtap="__e">{{user.username}}</text></view></view><view class="body"><view class="body_line"><text class="body_line_text">{{"学院："+user.deptName}}</text></view><view class="body_line"><text class="body_line_text">{{"学号："+user.account}}</text></view><view class="body_line"><text class="body_line_text">{{"姓名："+user.name}}</text></view><navigator class="body_line" url="email"><text class="body_line_text">{{"邮箱："+user.email}}</text><view class="body_line_icon">></view></navigator><navigator class="body_line" url="password"><text class="body_line_text">更改密码</text><view class="body_line_icon">></view></navigator><navigator class="body_line" style="margin-top:30rpx;" url="post"><text class="body_line_text">我的帖子</text><view class="body_line_icon">></view></navigator><view class="body" style="margin-top:30rpx;width:100%;"><view class="body_line"><text data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="body_line_text" bindtap="__e">退出登录</text></view><navigator class="body_line" url="/pages/login/login?to=me"><text class="body_line_text">切换账号</text></navigator></view></view></view>