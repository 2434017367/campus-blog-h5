page{
		height: 100%;
		width: 100%;
		background-color: rgb(246, 246, 246);
}
.main{
	height: 100%;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.header{
	width: 100%;
	height: 500rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	position: relative;
}
.header_image{
	width: 100%;
	height: 100%;
	position: absolute;
}
.head{
	width: 300rpx;
	height: 300rpx;
	/* margin-top: 60upx; */
	top: 100rpx;
	position: absolute;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.head_icon{
	width: 200rpx;
	height: 200rpx;
	border-radius: 200rpx;
	border-style: solid;
	border-width: 1px;
	border-color: #FFFFFF;
	background-color: #4CD964;
}
.head_font{
	font-family: "Comic Sans MS", cursive, sans-serif;
	font-size: 38rpx;
}
.body{
	margin-top: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.body_line{
	background-color: #FFFFFF;
	width: 100%;
	height: 90rpx;
	border-bottom: solid rgb(246, 246, 246) 2rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.body_line_text{
	line-height: 90rpx;
	font-size: 30rpx;
	margin-left: 35rpx;
}
.body_line_icon{
	line-height: 90rpx;
	font-size: 30rpx;
	margin-right: 35rpx;
}
.btns{
	margin-top: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
}
button{
	width: 300rpx;
}

