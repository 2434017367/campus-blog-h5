<view class="main"><view class="header"><navigator class="header_image" url="head_image?type=2"><image class="header_image" src="{{user.backImage}}"></image></navigator><view class="head"><navigator url="head_image?type=1"><image class="head_icon" src="{{user.headImage}}"></image></navigator><navigator url="username"><text class="head_font">{{user.username}}</text></navigator></view></view><view class="body"><view class="body_line"><text class="body_line_text">{{"学院："+user.deptName}}</text></view><view class="body_line"><text class="body_line_text">{{"学号："+user.account}}</text></view><view class="body_line"><text class="body_line_text">{{"姓名："+user.name}}</text></view><navigator class="body_line" url="email"><text class="body_line_text">{{"邮箱："+user.email}}</text><view class="body_line_icon">></view></navigator><navigator class="body_line" url="password"><text class="body_line_text">更改密码</text><view class="body_line_icon">></view></navigator><view class="body" style="margin-top:30rpx;width:100%;"><view class="body_line"><text data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="body_line_text" bindtap="__e">退出登录</text></view><navigator class="body_line" url="/pages/login/login?to=me"><text class="body_line_text">切换账号</text></navigator></view></view></view>